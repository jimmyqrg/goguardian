{
  "manifest_version": 3,
  "name": "GoGuardian",
  "version": "1.0",
  "description": "The most powerful classroom management tool for schools.",

  "background": {
    "service_worker": "sw.js"
  },

  "permissions": ["activeTab", "scripting"],

  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },

  "action": {
    "default_icon": "favicon.png",
    "default_title": "GoGuardian"
  }
}
